{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "noEmit": false,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["scripts/**/*"]
}
